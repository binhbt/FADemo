<?xml version="1.0" encoding="utf-8" ?>
<resources>
    <declare-styleable name="CircularImageView">
        <attr name="border" format="boolean"></attr>
        <attr name="border_width" format="integer"></attr>
        <attr name="border_color" format="color"></attr>
        <attr name="shadow" format="boolean"></attr>
    </declare-styleable>

    <declare-styleable name="AutoResizeKeepRatio">
        <attr name="ScaleHeight" format="integer"></attr>
        <attr name="ScaleWidth" format="integer"></attr>
        <attr name="ScaleBaseHeight" format="boolean"></attr>
        <attr name="disableAutoScaleImage" format="boolean"></attr>
    </declare-styleable>
    <declare-styleable name="Theme">
        <attr name="circularImageViewStyle" format="reference" />
    </declare-styleable>
</resources>